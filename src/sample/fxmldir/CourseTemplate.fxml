<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<fx:root prefHeight="76.0" prefWidth="293.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CourseTemplateController">
   <Region prefHeight="79.0" prefWidth="50.0" />
   <VBox prefHeight="76.0" prefWidth="243.0">
      <children>
          <Label fx:id="courseName" text="Course Name">
            <font>
               <Font name="Forte" size="23.0" />
            </font>
         </Label>
         <HBox prefHeight="18.0" prefWidth="208.0">
            <children>
                <Label fx:id="courseTheme" style="-fx-text-fill: gray;" text="CourseTheme">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
               <Region prefHeight="32.0" prefWidth="17.0" />
                <Label fx:id="courseLevel" style="-fx-text-fill: gray;" text="CourseLevel">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
            </children>
         </HBox>
         <HBox prefHeight="100.0" prefWidth="200.0">
            <children>
                <Label style="-fx-text-fill: gray;" text="Excercises: ">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
               <Region prefHeight="32.0" prefWidth="17.0" />
               <Label fx:id="courseExcerciseCount" prefHeight="18.0" prefWidth="100.0" text="ExcerciseCount">
                  <font>
                     <Font size="14.0" />
                  </font>
               </Label>
            </children>
         </HBox>
      </children>
   </VBox>
    <Button fx:id="open_button" prefHeight="79.0" prefWidth="106.0" style="-fx-background-color: #F39C63;" text="Open" textFill="WHITE">
      <font>
         <Font name="Forte" size="20.0" />
      </font></Button>
</fx:root>
